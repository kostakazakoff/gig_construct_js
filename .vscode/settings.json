    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "git.terminalAuthentication": false,
    "terminal.integrated.env.windows": {
        "GIT_SSH_COMMAND": "ssh -i ~/.ssh/id_ed25519"
