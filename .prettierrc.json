{
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "printWidth": 120,
    "overrides": [
        {
            "files": ["*.jsx", "*.tsx"],
            "options": {
                "tabWidth": 4
            }
        }
    ]
}
